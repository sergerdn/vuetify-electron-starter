{
  "extends": "./tsconfig.json",
  "include": ["src/electron-preload/**/*"],
  "compilerOptions": {
    "composite": true,
    "noEmit": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.preload.tsbuildinfo",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": ["node"],
    "lib": ["ES2022", "DOM"],
    "skipLibCheck": true
  }
}
